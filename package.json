    "infinite-react-carousel": "^1.2.9",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-awesome-slider": "^4.0.2",
    "react-burger-menu": "^2.6.13",
    "react-dom": "^16.9.0",
    "react-hamburger-menu": "^1.1.1",
    "react-helmet": "^5.2.1",
    "react-responsive-carousel": "^3.1.51",
